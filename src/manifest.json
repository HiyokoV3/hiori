{
  "manifest_version": 2,
  "name": "Hiori",
  "short_name": "hiorisc",
  "description": "Shiny colors",
  "version": "1.0.0",
  "icons": {
    "16": "img/16.png",
    "64": "img/64.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://shinycolors.enza.fun/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [ "*://shinycolors.enza.fun/*" ],
      "js": [ "content.js" ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "injects.js"
  ]
}
